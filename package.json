{
  "name": "remote-redux-devtools-on-debugger",
  "version": "0.4.4",
  "description": "Injecting remote-redux-devtools / remotedev monitor into react-native debugger",
  "main": "bin/main.js",
  "files": [
    "bin",
    "bundle.js"
  ],
  "bin": {
    "remotedev-debugger": "bin/cli.js"
  },
  "scripts": {
    "lint": "eslint bin/ app/ test/*.js webpack.*",
    "test": "mocha --compilers js:babel-core/register -r test/setup test/*.test.js",
    "clean": "rm bundle.js",
    "build:webpack": "webpack --stats --progress",
    "build": "npm run build:webpack",
    "dev": "webpack-dev-server --config=webpack.config.dev.js --host=localhost --hot --inline --colors",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhen0409/remote-redux-devtools-on-debugger.git"
  },
  "keywords": [
    "remote",
    "redux",
    "devtools",
    "remotedev",
    "react",
    "native",
    "debugger",
    "desktop"
  ],
  "authors": [
    "Jhen <developer@jhen.me> (https://github.com/jhen0409)",
    "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "fs-extra": "^0.30.0",
    "mocha": "^2.4.5",
    "react": "^15.0.2",
    "react-dock": "^0.2.3",
    "react-dom": "^15.0.2",
    "remotedev-app": "^0.3.8",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "getport": "^0.1.0",
    "minimist": "^1.2.0",
    "remotedev-server": "^0.1.0"
  },
  "peerDependencies": {
    "remote-redux-devtools": ">= 0.2.0"
  }
}
